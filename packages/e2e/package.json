{
  "name": "@recipe-manager/e2e",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:headless": "cypress run --headless",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:edge": "cypress run --browser edge",
    "test": "cypress run",
    "test:headless": "cypress run --headless",
    "test:smoke": "cypress run --spec 'cypress/e2e/smoke/**/*'",
    "test:auth": "cypress run --spec 'cypress/e2e/auth/**/*'",
    "test:recipes": "cypress run --spec 'cypress/e2e/recipes/**/*'",
    "test:critical": "cypress run --spec 'cypress/e2e/critical-path/**/*'",
    "test:regression": "cypress run --spec 'cypress/e2e/regression/**/*'",
    "test:parallel": "cypress run --record --parallel",
    "verify": "cypress verify",
    "info": "cypress info",
    "lint": "eslint . --ext .ts,.js --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc --noEmit --strict --noUnusedLocals --noUnusedParameters --exactOptionalPropertyTypes --noImplicitReturns --noUncheckedIndexedAccess"
  },
  "dependencies": {
    "@recipe-manager/shared": "file:../shared"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.13.4",
    "@faker-js/faker": "^9.9.0",
    "@types/node": "^20.11.24",
    "cypress": "^13.17.0",
    "cypress-axe": "^1.6.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-image-diff-js": "^2.2.0",
    "cypress-mailhog": "^1.7.0",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-real-events": "^1.13.0",
    "junit": "^1.4.9",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
